<div id="deleteCallbackModal_{{id}}" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h3 id="myModalLabel">{% trans from "admin" %}Are you sure?{% endtrans %}</h3>
    </div>
    <div class="modal-body">
        <p><i class="icon-warning-sign"></i> {% trans with {'%id%' : id } from "admin" %}The callback "ID" = %id% will be removed from the list.{% endtrans %}</p>
    </div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">
            <i class="icon-remove"></i> {% trans from "admin" %}Cansel{% endtrans %}</button>
        <button class="btn btn-danger" onclick="FormAction.deleteContent({{id}})">
            <i class="icon-trash icon-white"></i> {% trans from "admin" %}Yes delete{% endtrans %}
        </button>
    </div>
</div>
