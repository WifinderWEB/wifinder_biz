{% extends '::base.html.twig' %}

{% block body %}
    <div class="row-fluid">
        {% block breadCrumb %}
            {% render(controller( "FrontendContentBundle:Menu:breadCrumb", {"alias" : content.alias })) %}
        {% endblock %}
    </div>
    <div class="row-fluid">
        <div class="span12" id="main_content">

            <h1>{% block titlePage%}{% endblock %}</h1>

            {% block content %}{% endblock %}
        </div>
    </div>
{% endblock %}
